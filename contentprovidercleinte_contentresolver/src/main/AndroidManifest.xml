<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdc.curso.contentprovidercleinte_contentresolver" >

    <!--Se lo indicamos aqui-->
    <uses-permission android:name="com.sdc.curso.contentprovider.NOTICIAS_PROVIDER_READ"/>
    <uses-permission android:name="com.sdc.curso.contentprovider.NOTICIAS_PROVIDER_WRITE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
